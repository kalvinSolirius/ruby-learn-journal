<h1 class="text-center py-3">All Entries</h1>

<div class="container">

      <table class="table">
          <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Link</th>
            <th scope="col"></th>
          </tr>
          </thead>
        <% @entries.each do |entry| %>
          <tbody>
          <tr>
            <td><%= entry.name %></td>
            <td><%= entry.link %></td>
            <td><%= button_to "Delete", entry, method: :delete, data: { confirm: "Are you sure you want to delete this Project ?"}, class: "btn btn-primary" %></td>
          </tr>
          </tbody>
        <% end %>

      </table>
</div>